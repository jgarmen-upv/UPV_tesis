  groups        = {Corregidos, Collaboration},
  keywords      = {Neutrino telescope, Undersea Cherenkov detectors, Sea water properties: absorption and transmission of light},
  url           = {https://www.sciencedirect.com/science/article/pii/S0927650504001902},
}

